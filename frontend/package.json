{
  "name": "angular-material-app-with-dynamic-themes",
  "version": "0.0.0",
  "description": "An Angular app that can switch between material-themes at run-time",
  "scripts": {
    "ng": "ng",
    "prestart": "koji-tools watch &",
    "start": "cross-env NODE_ENV=development ng serve --host 0.0.0.0 --port 4200 --disableHostCheck=true",
    "build": "cross-env NODE_ENV=production ng build --prod --deleteOutputPath=false",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "engines": {
    "node": ">=10.16 <= 12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mirismaili/AngularMaterialDynamicThemesScaffold.git"
  },
  "keywords": [
    "angular",
    "angular-material",
    "angular-material-theme",
    "angular-material-themes",
    "theme",
    "themes",
    "dynamic-theme",
    "dynamic-themes",
    "runtime-theme-change",
    "runtime-change-themes",
    "sass",
    "scss",
    "template",
    "scaffold",
    "starter",
    "starter-kit",
    "koji"
  ],
  "author": "S. M. Mir-Ismaili <s.m.mirismaili@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mirismaili/AngularMaterialDynamicThemesScaffold/issues"
  },
  "homepage": "https://github.com/mirismaili/AngularMaterialDynamicThemesScaffold#readme",
  "private": true,
