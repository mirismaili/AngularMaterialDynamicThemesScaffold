{
  "name": "typescript-backend",
  "version": "1.0.0",
  "description": "typescript server",
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=development ts-node-dev --respawn --transpileOnly ./index.ts",
    "start:watch": "nodemon",
    "prod": "cross-env NODE_ENV=production npm run build"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "express": "4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.0",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.8",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.39",
    "typescript": "^3.2.2"
  }
}
